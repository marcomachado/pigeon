<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="fill_parent"
	android:layout_width="fill_parent"
	android:background="@drawable/mosaic_pigeon_img_layer_background_large">

	<TableLayout
		android:id="@+id/TableLayout02"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<TableRow
			android:layout_gravity="center_horizontal">
			<LinearLayout
				android:id="@+id/LinearLayoutPos1">
			</LinearLayout>
			<LinearLayout
				android:id="@+id/LinearLayoutPos6"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@drawable/picture_of_avatar"
				android:orientation="vertical"
				android:layout_marginLeft="150px"
				android:layout_marginTop="4px"
				android:layout_marginRight="4px">
				<ImageView
					android:id="@+id/ImageViewPos6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/paulo"
					android:layout_margin="6px"></ImageView>
				<TextView
					android:text="@string/pos1"
					android:id="@+id/TextView01"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"></TextView>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/LinearLayoutPos2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@drawable/picture_of_avatar"
				android:orientation="vertical"
				android:layout_margin="4px">
				<ImageView
					android:id="@+id/ImageViewPos2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/dhiego"
					android:layout_margin="6px"></ImageView>
				<TextView
					android:text="@string/pos2"
					android:id="@+id/TextView01"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"></TextView>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/LinearLayoutPos3"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@drawable/picture_of_avatar"
				android:orientation="vertical"
				android:layout_margin="4px">
				<ImageView
					android:id="@+id/ImageViewPos3"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/fernanda"
					android:layout_margin="6px"></ImageView>
				<TextView
					android:text="@string/pos3"
					android:id="@+id/TextView01"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"></TextView>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/LinearLayoutPos4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@drawable/picture_of_avatar"
				android:orientation="vertical"
				android:layout_margin="4px">
				<ImageView
					android:id="@+id/ImageViewPos4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:background="@drawable/lenin"
					android:layout_margin="6px"></ImageView>
				<TextView
					android:text="@string/pos4"
					android:id="@+id/TextView01"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"></TextView>
			</LinearLayout>
			<LinearLayout
				android:layout_margin="4px"
				android:background="@drawable/picture_of_avatar"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:id="@+id/LinearLayoutPos5"
				android:layout_width="wrap_content">
				<ImageView
					android:layout_margin="6px"
					android:id="@+id/ImageViewPos5"
					android:background="@drawable/joao"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"></ImageView>
				<TextView
					android:text="@string/pos5"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_gravity="center_horizontal"
					android:id="@+id/TextView01"></TextView>
			</LinearLayout>
		</TableRow>

		<TableRow
			android:layout_height="fill_parent">
			<TableLayout
				android:id="@+id/TableLayout01"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<TableRow>
					<TableLayout
						android:id="@+id/TableLayout03"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content">

						<TableRow>
							<Button
								android:background="@drawable/facebook"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginBottom="10dip"
								android:id="@+id/social_confg"
								android:layout_gravity="top|center" />
						</TableRow>
						<TableRow>
							<Button
								android:layout_width="wrap_content"
								android:layout_marginBottom="10dip"
								android:layout_height="wrap_content"
								android:text="@string/top_five"
								android:id="@+id/top_five"></Button>
						</TableRow>
						<TableRow>
							<Button
								android:layout_height="wrap_content"
								android:id="@+id/high_score"
								android:layout_width="wrap_content"
								android:text="@string/high_score"></Button>
						</TableRow>
					</TableLayout>
				</TableRow>
			</TableLayout>
		</TableRow>

		<RelativeLayout
			android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			<ImageButton
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:background="@drawable/mosaic_pigeon_img_layer_bnt_start_large"
				android:id="@+id/start_game"
				android:layout_alignParentTop="true"
				android:layout_centerHorizontal="true"
				></ImageButton>
		</RelativeLayout>


	</TableLayout>
</LinearLayout>