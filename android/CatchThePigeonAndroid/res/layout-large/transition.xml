			android:layout_y="20dip"></ImageView>
		<ImageView
			android:id="@+id/transition_img_tf2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/mosaic_pigeon_ima_quadro_foto"
			android:layout_x="494dip"
			android:layout_y="20dip"></ImageView>
		<TextView
			android:text="@string/pos1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/transition_score_tf1"
			android:layout_x="425dip"
			android:layout_y="82dip" 
			android:textStyle="bold"></TextView>
		<TextView
			android:text="@string/pos2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/transition_score_tf2"
			android:layout_x="505dip"
			android:layout_y="81dip"
			android:textStyle="bold"></TextView>
		<TextView
			android:text="@string/pos3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/transition_score_tf3"
			android:layout_x="585dip"
			android:layout_y="81dip"
			android:textStyle="bold"></TextView>
		<ImageView
			android:id="@+id/transition_img_tf4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/mosaic_pigeon_ima_quadro_foto"
			android:layout_x="415dip"
			android:layout_y="132dip"></ImageView>
		<ImageView
			android:id="@+id/transition_img_tf5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/mosaic_pigeon_ima_quadro_foto"
			android:layout_x="494dip"
			android:layout_y="132dip"></ImageView>
		<TextView
			android:text="@string/pos4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/transition_score_tf4"
			android:layout_x="425dip"
			android:layout_y="194dip"
			android:textStyle="bold"></TextView>
		<TextView
			android:text="@string/pos5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/transition_score_tf5"
			android:layout_x="505dip"
			android:layout_y="193dip"
			android:textStyle="bold"></TextView>
		<ImageButton
			android:background="@drawable/mosaic_pigeon_img_layer_help"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/help_button_transition"
			android:layout_x="568dip"
			android:layout_y="240dip"></ImageButton>
		<ImageButton
			android:background="@drawable/mosaic_pigeon_img_layer_facebook"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/facebook_button_transition"
			android:layout_x="10dip"
			android:layout_y="162dip"></ImageButton>
		<ImageButton
			android:background="@drawable/mosaic_pigeon_img_layer_twitter"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/twitter_button_transition"
			android:layout_x="10dip"
			android:layout_y="241dip"></ImageButton>
		<ImageButton
			android:background="@drawable/mosaic_pigeon_img_layer_back"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/back_button"
			android:layout_x="8dip"
			android:layout_y="343dip"></ImageButton>
		<ImageButton
			android:background="@drawable/mosaic_pigeon_icon_audio_icon"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/audio_button_transition"
			android:layout_x="71dip"
			android:layout_y="341dip"></ImageButton>
		<TextView android:textSize="20pt" 
			android:text="2" 
			android:textStyle="bold" 
			android:layout_height="wrap_content" 
			android:id="@+id/transition_level" 
			android:typeface="normal" 
			android:layout_width="wrap_content" 
			android:textColor="#A0522D" 
			android:layout_x="322dip" 
			android:layout_y="177dip"></TextView>
		<TextView android:textSize="9.5pt" 
			android:text="Level" 
			android:textStyle="bold" 
			android:layout_height="wrap_content" 
			android:id="@+id/textView2" 
			android:typeface="sans" 
			android:layout_width="wrap_content" 
			android:textColor="#A0522D" 
			android:layout_x="258dip" 
			android:layout_y="201dip"></TextView>
		<TextView android:textSize="10pt" 
			android:text="50000 pt" 
			android:textStyle="bold" 
			android:layout_height="wrap_content" 
			android:id="@+id/transition_score" 
			android:typeface="sans" 
			android:layout_width="wrap_content" 
			android:textColor="#A0522D"
			android:layout_x="258dip" 
			android:layout_y="25dip"></TextView>
		
	</AbsoluteLayout>


</AbsoluteLayout>
