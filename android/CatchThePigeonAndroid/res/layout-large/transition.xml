<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<AbsoluteLayout
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:background="@drawable/mosaic_pigeon_default_bg"
		android:id="@+id/absoluteLayout1"
		android:layout_x="0dip"
		android:layout_y="0dip">
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/ImageViewPos6"
			android:background="@drawable/mosaic_pigeon_tela_transicao_separador_botoes"
			android:layout_x="305dip"
			android:layout_y="243dip"></ImageView>
		<ImageButton
			android:layout_width="wrap_content"
			android:id="@+id/next_level"
			android:layout_height="wrap_content"
			android:background="@drawable/mosaic_pigeon_tela_transicao_go"
			android:layout_x="360dip"
			android:layout_y="243dip"></ImageButton>
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/ImageViewPos6"
			android:background="@drawable/mosaic_pigeon_img_layer_picture_of_avatar_large"
			android:layout_x="570dip"
			android:layout_y="14dip"></ImageView>
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/ImageViewPos6"
			android:background="@drawable/mosaic_pigeon_img_layer_picture_of_avatar_large"
			android:layout_x="490dip"
			android:layout_y="126dip"></ImageView>
		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/ImageViewPos6"
