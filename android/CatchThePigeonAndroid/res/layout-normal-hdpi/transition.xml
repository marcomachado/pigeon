		<TextView android:textSize="9.5pt" 
			android:text="50000 pt" 
			android:textStyle="bold" 
			android:layout_height="wrap_content" 
			android:id="@+id/transition_score" 
			android:typeface="sans" 
			android:layout_width="wrap_content" 
			android:textColor="#A0522D" 
			android:layout_x="228dip" 
			android:layout_y="18dip"></TextView>
		<TextView android:textSize="16pt" 
			android:text="2" 
			android:textStyle="bold" 
			android:layout_height="wrap_content" 
			android:id="@+id/transition_level" 
			android:typeface="normal" 
			android:layout_width="wrap_content" 
			android:textColor="#A0522D" 
			android:layout_x="286dip" 
			android:layout_y="131dip"></TextView>
		<TextView android:textSize="8pt" 
			android:text="Level" 
			android:textStyle="bold" 
			android:layout_height="wrap_content" 
			android:id="@+id/textView2" 
			android:typeface="sans" 
			android:layout_width="wrap_content" 
			android:textColor="#A0522D" 
			android:layout_x="236dip" 
