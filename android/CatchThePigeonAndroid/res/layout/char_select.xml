<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_height="fill_parent" android:layout_width="fill_parent" android:background="@drawable/background">
	<TableLayout  
		android:id="@+id/TableLayout02" 
		android:layout_width="fill_parent" 
		android:layout_height="fill_parent" android:background="@drawable/background_character_selection">
		
		<TableRow>
			<LinearLayout android:id="@+id/LinearLayoutPos6" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="vertical">
				<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/sigeon_noselection" android:id="@+id/sigeon_button"></Button>
			</LinearLayout>
			<LinearLayout android:id="@+id/LinearLayoutPos3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="vertical" android:layout_margin="10px">
				<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/figean_noselection" android:id="@+id/sigean_button"></Button>
			</LinearLayout>
			<LinearLayout android:id="@+id/LinearLayoutPos5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal">
				<Button android:background="@drawable/figeon_noselection" android:layout_width="wrap_content" android:layout_weight="1" android:id="@+id/figeon_button" android:layout_height="wrap_content"></Button>
			</LinearLayout>			
</TableRow>
		<TableRow android:id="@+id/tableRow1" android:layout_height="fill_parent" android:layout_width="fill_parent">
			<LinearLayout android:id="@+id/linearLayout3" android:layout_height="fill_parent" android:layout_width="match_parent" android:gravity="fill_horizontal">
				<Button android:layout_width="wrap_content" android:height="40px" android:id="@+id/start_game" android:background="@drawable/bnt_start" android:enabled="true" android:width="40px" android:layout_height="wrap_content" android:layout_gravity="center|fill"></Button>
			</LinearLayout>
		</TableRow>
		<TableRow android:id="@+id/tableRow3" android:layout_height="fill_parent" android:layout_width="wrap_content" android:layout_gravity="bottom|fill">
			<TableLayout android:id="@+id/TableLayout01" android:layout_height="wrap_content" android:layout_width="wrap_content">
				<TableRow android:id="@+id/tableRow4" android:layout_height="fill_parent" android:layout_width="wrap_content">
					<TableLayout android:id="@+id/TableLayout03" android:layout_height="wrap_content" android:layout_width="wrap_content">
						<TableRow android:id="@+id/tableRow5" android:layout_width="wrap_content" android:layout_height="wrap_content">
							<Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/facebook_config" android:layout_gravity="top|center" android:background="@drawable/mosaic_pigeon_img_layer_facebook_320" android:layout_marginBottom="10dip"></Button>
						</TableRow>
						<TableRow android:id="@+id/tableRow6" android:layout_width="wrap_content" android:layout_height="wrap_content">
							<Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/twitter_config" android:layout_gravity="top|center" android:background="@drawable/twitter" android:layout_marginBottom="10dip"></Button>
						</TableRow>
					</TableLayout>
				</TableRow>
			</TableLayout>
			<LinearLayout android:layout_height="fill_parent" android:layout_width="fill_parent" android:id="@+id/linearLayout2">
				<Button android:layout_width="wrap_content" android:id="@+id/back" android:background="@drawable/back" android:width="30px" android:layout_gravity="center" android:layout_height="wrap_content"></Button>
				<Button android:layout_width="wrap_content" android:id="@+id/sound" android:background="@drawable/mosaic_pigeon_img_layer_play_320" android:width="30px" android:layout_gravity="center" android:layout_height="wrap_content"></Button>
			</LinearLayout>
			<LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/linearLayout1">
				<Button android:layout_width="wrap_content" android:id="@+id/help_button" android:background="@drawable/mosaic_pigeon_img_layer_help" android:width="30px" android:layout_height="wrap_content"></Button>
			</LinearLayout>
		</TableRow>
		
	
	</TableLayout>
</LinearLayout>